# Test SELECT ... HAVING with COMPLEX type
# Grammar: SELECT ... HAVING ...

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t1 (val COMPLEX);
INSERT INTO t1 VALUES ('(1.0,2.0)'), ('(1.0,2.0)'), ('(3.0,4.0)');

SELECT val, COUNT(*) as c FROM t1 GROUP BY val HAVING c > 1;

DROP TABLE t1;

--source include/villagesql/uninstall_complex_extension.inc
