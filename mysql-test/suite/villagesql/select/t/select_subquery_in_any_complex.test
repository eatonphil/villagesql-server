# Test Subquery with IN/ANY/SOME for COMPLEX type
# Grammar: SELECT ... WHERE expr IN (SELECT ...) / expr = ANY (SELECT ...)

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t1 (id INT PRIMARY KEY, val COMPLEX);
INSERT INTO t1 VALUES (1, '(1.0,2.0)'), (5, '(0,-0)'), (2, '(3.0,4.0)'), (4, '(0,0)'), (3, '(5.0,6.0)');

CREATE TABLE t2 (filter_val COMPLEX);
INSERT INTO t2 VALUES ('(1.0,2.0)'), ('(5.0,6.0)'), ('(-0,-0)');

# IN subquery
SELECT * FROM t1 WHERE val IN (SELECT filter_val FROM t2) ORDER BY id;

# = ANY subquery (equivalent to IN)
SELECT * FROM t1 WHERE val = ANY (SELECT filter_val FROM t2) ORDER BY val, id;

DROP TABLE t1;
DROP TABLE t2;

--source include/villagesql/uninstall_complex_extension.inc
