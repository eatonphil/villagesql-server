# Test ALTER TABLE ADD multiple columns including COMPLEX type
# Grammar: ALTER TABLE table_name ADD (column_list)

--source include/villagesql/install_complex_extension.inc
--source include/villagesql/binlog_check_begin.inc

CREATE TABLE t1 (
    id INT PRIMARY KEY
);

--source include/villagesql/query_custom_columns.inc

SHOW CREATE TABLE t1;

ALTER TABLE t1 ADD (
    name VARCHAR(50),
    value COMPLEX,
    score DOUBLE,
    value2 COMPLEX
);

--source include/villagesql/query_custom_columns.inc

SHOW CREATE TABLE t1;

INSERT INTO t1 VALUES (1, 'test1', '(1.0,2.0)', 95.5, '(2.0,1.0)');
INSERT INTO t1 VALUES (2, 'test2', '(3.5,-1.2)', 87.3, '(-1.2,3.5)');

SELECT * FROM t1;

DROP TABLE t1;

--source include/villagesql/query_custom_columns.inc

--source include/villagesql/binlog_check_end.inc
--source include/villagesql/uninstall_complex_extension.inc
