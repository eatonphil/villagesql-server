# Test ALTER TABLE RENAME COLUMN on COMPLEX column
# Grammar: ALTER TABLE table_name RENAME COLUMN old_name TO new_name

--source include/villagesql/install_complex_extension.inc
--source include/villagesql/binlog_check_begin.inc

CREATE TABLE t1 (
    id INT PRIMARY KEY,
    old_complex COMPLEX
);

--source include/villagesql/query_custom_columns.inc

INSERT INTO t1 VALUES (1, '(1.0,2.0)');
INSERT INTO t1 VALUES (2, '(3.5,-1.2)');

SHOW CREATE TABLE t1;
SELECT * FROM t1;

ALTER TABLE t1 RENAME COLUMN old_complex TO new_complex;

--source include/villagesql/query_custom_columns.inc

SHOW CREATE TABLE t1;
SELECT * FROM t1;

DROP TABLE t1;

--source include/villagesql/query_custom_columns.inc

--source include/villagesql/binlog_check_end.inc
--source include/villagesql/uninstall_complex_extension.inc
