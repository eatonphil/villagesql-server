# Test ALTER TABLE MODIFY COLUMN to COMPLEX type
# Grammar: ALTER TABLE table_name MODIFY COLUMN column_name COMPLEX

--source include/villagesql/not_implemented_complex_type.inc

--source include/villagesql/install_complex_extension.inc
--source include/villagesql/binlog_check_begin.inc

CREATE TABLE t1 (
    id INT PRIMARY KEY,
    value VARCHAR(50)
);

--source include/villagesql/query_custom_columns.inc

INSERT INTO t1 VALUES (1, 'some_text');
INSERT INTO t1 VALUES (2, 'other_data');

SHOW CREATE TABLE t1;
SELECT * FROM t1;

ALTER TABLE t1 MODIFY COLUMN value COMPLEX;

--source include/villagesql/query_custom_columns.inc

SHOW CREATE TABLE t1;

# Update with COMPLEX values after type change
UPDATE t1 SET value = '(1.0,2.0)' WHERE id = 1;
UPDATE t1 SET value = '(3.5,-1.2)' WHERE id = 2;

SELECT * FROM t1;

DROP TABLE t1;

--source include/villagesql/query_custom_columns.inc

--source include/villagesql/binlog_check_end.inc
--source include/villagesql/uninstall_complex_extension.inc
