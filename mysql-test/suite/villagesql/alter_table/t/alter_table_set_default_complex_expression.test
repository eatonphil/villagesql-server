# Test ALTER TABLE ALTER COLUMN SET DEFAULT with COMPLEX expression
# Grammar: ALTER TABLE table_name ALTER COLUMN col_name SET DEFAULT (complex_expression)

--source include/villagesql/not_implemented_complex_type.inc

--source include/villagesql/install_complex_extension.inc
--source include/villagesql/binlog_check_begin.inc

CREATE TABLE t1 (
    id INT PRIMARY KEY,
    value COMPLEX
);

INSERT INTO t1 VALUES (1, '(1.0,2.0)');

SHOW CREATE TABLE t1;

# Test setting default to a complex expression
ALTER TABLE t1 ALTER COLUMN value SET DEFAULT ('(0.0,1.0)');

SHOW CREATE TABLE t1;

# Test that new rows get the default
INSERT INTO t1 (id) VALUES (2);

SELECT * FROM t1;

DROP TABLE t1;

--source include/villagesql/binlog_check_end.inc
--source include/villagesql/uninstall_complex_extension.inc
