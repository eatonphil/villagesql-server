CREATE TABLE t_complex (id INT PRIMARY KEY, val COMPLEX);
INSERT INTO t_complex VALUES (1, '(1.0,2.0)');
CREATE TEMPORARY TABLE temp_complex (id INT, val COMPLEX);
INSERT INTO temp_complex SELECT * FROM t_complex WHERE id = 1;
SELECT * FROM temp_complex;
id	val
1	(1,2)
SELECT id, vsql_complex.complex_conjugate(val) as conjugate FROM temp_complex;
id	conjugate
1	(1,-2)
DROP TABLE t_complex;
DROP TEMPORARY TABLE temp_complex;
