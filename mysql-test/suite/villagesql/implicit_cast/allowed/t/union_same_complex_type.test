# Test UNION of same COMPLEX type

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t_complex (id INT PRIMARY KEY, val COMPLEX);

INSERT INTO t_complex VALUES
  (1, '(1.0,2.0)'),
  (2, '(3.0,4.0)'),
  (3, '(5.0,6.0)');

CREATE TABLE t_complex2 (val COMPLEX);

INSERT INTO t_complex2 VALUES ('(1.0,2.0)'), ('(0.0,0.0)');

SELECT val FROM t_complex
UNION
SELECT val FROM t_complex2
ORDER BY val;

SELECT val FROM t_complex WHERE id = 1
UNION
SELECT val FROM t_complex WHERE id = 2
ORDER BY val DESC;

DROP TABLE t_complex;
DROP TABLE t_complex2;

--source include/villagesql/uninstall_complex_extension.inc
