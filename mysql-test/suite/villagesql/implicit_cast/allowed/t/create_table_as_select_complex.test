# Test CREATE TABLE AS SELECT with COMPLEX (same type)

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t_complex (id INT PRIMARY KEY, val COMPLEX);

INSERT INTO t_complex VALUES
  (1, '(1.0,2.0)'),
  (2, '(3.0,4.0)');

CREATE TABLE test_ctas AS
SELECT id, val FROM t_complex WHERE id <= 2;

SELECT * FROM test_ctas ORDER BY id;

DROP TABLE t_complex;
DROP TABLE test_ctas;

--source include/villagesql/uninstall_complex_extension.inc
