# Test COALESCE with COMPLEX type
# COALESCE does not support implicit conversion with custom types

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t1 (val COMPLEX);
INSERT INTO t1 VALUES ('(1.0,2.0)'), (NULL);

# COALESCE with implicit type conversion - not supported
--error ER_WRONG_USAGE
SELECT COALESCE(val, '(0.0,0.0)') FROM t1;

DROP TABLE t1;

--source include/villagesql/uninstall_complex_extension.inc
