# Test INSERT ON DUPLICATE KEY UPDATE with same type (newer syntax)

--source include/villagesql/not_implemented_complex_type.inc
--source include/villagesql/install_complex_extension.inc

CREATE TABLE t_complex (id INT PRIMARY KEY, val COMPLEX);

INSERT INTO t_complex VALUES (1, '(13.0,14.0)');

INSERT INTO t_complex VALUES (1, '(14.0, 15.0)') AS new_tuple
ON DUPLICATE KEY UPDATE val = new_tuple.val;
SELECT val FROM t_complex WHERE id = 1;

DROP TABLE t_complex;

--source include/villagesql/uninstall_complex_extension.inc
