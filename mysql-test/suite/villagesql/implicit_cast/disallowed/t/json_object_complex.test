# Test JSON_OBJECT with COMPLEX type
# JSON_OBJECT does not support custom types

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t1 (id INT, val COMPLEX);
INSERT INTO t1 VALUES (1, '(1.0,2.0)');

# JSON_OBJECT with COMPLEX value - not supported
--error ER_WRONG_USAGE
SELECT JSON_OBJECT('result', val) FROM t1;

DROP TABLE t1;

--source include/villagesql/uninstall_complex_extension.inc
