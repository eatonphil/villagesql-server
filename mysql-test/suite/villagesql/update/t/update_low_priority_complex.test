# Test UPDATE LOW_PRIORITY ... with COMPLEX type
# Grammar: UPDATE LOW_PRIORITY ...

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t1 (
    id INT PRIMARY KEY,
    val COMPLEX
);

# Insert 10 rows with zero variants
INSERT INTO t1 VALUES
    (1, '(1.0,2.0)'),
    (2, '(0,0)'),
    (3, '(-3.5,4.5)'),
    (4, '(-0,0)'),
    (5, '(5.0,-6.0)'),
    (6, '(0,-0)'),
    (7, '(-7.5,-8.5)'),
    (8, '(-0,-0)'),
    (9, '(9.0,10.0)'),
    (10, '(0,0)');

# Low priority update single row
UPDATE LOW_PRIORITY t1 SET val = '(100.0,200.0)' WHERE id = 1;

SELECT * FROM t1 ORDER BY id;

# Low priority update multiple rows
UPDATE LOW_PRIORITY t1 SET val = '(50.0,60.0)' WHERE id IN (2, 4, 6, 8);

SELECT * FROM t1 ORDER BY id;

DROP TABLE t1;

--source include/villagesql/uninstall_complex_extension.inc
