# Test CREATE TEMPORARY TABLE IF NOT EXISTS with COMPLEX type
# Grammar: CREATE TEMPORARY TABLE IF NOT EXISTS table_name (column_list)

--source include/villagesql/install_complex_extension.inc
--source include/villagesql/binlog_check_begin.inc

CREATE TEMPORARY TABLE IF NOT EXISTS t1 (
    id INT PRIMARY KEY,
    temp_value COMPLEX
);

SHOW COLUMNS FROM t1;

INSERT INTO t1 VALUES (1, '(-1.5,2.8)');
INSERT INTO t1 VALUES (2, '(4.0,0.0)');

SELECT * FROM t1;

# Second create should not fail
CREATE TEMPORARY TABLE IF NOT EXISTS t1 (
    different_col INT
);

SELECT * FROM t1;

DROP TABLE t1;

--source include/villagesql/binlog_check_end.inc
--source include/villagesql/uninstall_complex_extension.inc
