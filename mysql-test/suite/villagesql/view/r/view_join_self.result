CREATE VIEW v_self_join AS
SELECT a.id AS id1, a.label AS label1, b.id AS id2, b.label AS label2, a.val
FROM t1 a JOIN t1 b ON a.val = b.val
WHERE a.id < b.id;
SELECT * FROM v_self_join ORDER BY id1, id2;
