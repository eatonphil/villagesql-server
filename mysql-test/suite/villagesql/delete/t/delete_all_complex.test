# Test DELETE FROM ... (all rows) with COMPLEX type
# Grammar: DELETE [FROM] tbl_name ...

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t1 (
    id INT PRIMARY KEY,
    val COMPLEX
);

# Insert 10 rows with zero variants in mixed order
INSERT INTO t1 VALUES
    (5, '(5.0,-6.0)'),
    (1, '(1.0,2.0)'),
    (8, '(-0,-0)'),
    (3, '(-3.5,4.5)'),
    (10, '(0,0)'),
    (2, '(0,0)'),
    (7, '(-7.5,-8.5)'),
    (4, '(-0,0)'),
    (9, '(9.0,10.0)'),
    (6, '(0,-0)');

SELECT * FROM t1 ORDER BY id;

# Delete all rows
DELETE FROM t1;

# Verify the table is empty two different ways
SELECT * FROM t1;
SELECT COUNT(*) FROM t1;

DROP TABLE t1;

--source include/villagesql/uninstall_complex_extension.inc
