# Test DELETE t1 FROM t1, t2 ... with COMPLEX type
# Grammar: DELETE table_references FROM ...

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t1 (
    id INT PRIMARY KEY,
    val COMPLEX
);

CREATE TABLE t2 (
    id INT PRIMARY KEY
);

# Insert 10 rows in t1 with zero variants
INSERT INTO t1 VALUES
    (1, '(1.0,2.0)'),
    (2, '(0,0)'),
    (3, '(-3.5,4.5)'),
    (4, '(-0,0)'),
    (5, '(5.0,-6.0)'),
    (6, '(0,-0)'),
    (7, '(-7.5,-8.5)'),
    (8, '(-0,-0)'),
    (9, '(9.0,10.0)'),
    (10, '(0,0)');

# Insert matching IDs in t2
INSERT INTO t2 VALUES (1), (3), (5), (7), (9);

SELECT * FROM t1 ORDER BY id;
SELECT * FROM t2 ORDER BY id;

# Multi-table delete (comma syntax) - delete from t1 where id matches t2
DELETE t1 FROM t1, t2 WHERE t1.id = t2.id;

SELECT * FROM t1 ORDER BY id;

DROP TABLE t1;
DROP TABLE t2;

--source include/villagesql/uninstall_complex_extension.inc
