# Test DELETE ... ORDER BY ... LIMIT ... with COMPLEX type
# Grammar: DELETE ... ORDER BY ... LIMIT ...

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t1 (
    id INT PRIMARY KEY,
    val COMPLEX
);

# Insert 10 rows with zero variants
INSERT INTO t1 VALUES
    (1, '(1.0,2.0)'),
    (2, '(0,0)'),
    (3, '(-3.5,4.5)'),
    (4, '(-0,0)'),
    (5, '(5.0,-6.0)'),
    (6, '(0,-0)'),
    (7, '(-7.5,-8.5)'),
    (8, '(-0,-0)'),
    (9, '(9.0,10.0)'),
    (10, '(0,0)');

SELECT * FROM t1 ORDER BY id;

# Delete with ORDER BY id DESC LIMIT
DELETE FROM t1 ORDER BY id DESC LIMIT 1;

SELECT * FROM t1 ORDER BY id;

# Delete with ORDER BY id ASC LIMIT
DELETE FROM t1 ORDER BY id ASC LIMIT 2;

SELECT * FROM t1 ORDER BY id;

# Delete with ORDER BY id DESC LIMIT 3
DELETE FROM t1 ORDER BY id DESC LIMIT 3;

SELECT * FROM t1 ORDER BY id;

DROP TABLE t1;

--source include/villagesql/uninstall_complex_extension.inc
