id INT,
complex_data COMPLEX,
partition_key INT,
PRIMARY KEY(id, partition_key)
) PARTITION BY RANGE (partition_key) (
PARTITION p0 VALUES LESS THAN (10),
PARTITION p1 VALUES LESS THAN (20),
PARTITION p2 VALUES LESS THAN MAXVALUE
);
