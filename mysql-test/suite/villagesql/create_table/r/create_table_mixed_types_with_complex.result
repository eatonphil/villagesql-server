CREATE TABLE mixed_types_table (
# Integer types
tiny_col TINYINT,
small_col SMALLINT,
medium_col MEDIUMINT,
int_col INT,
big_col BIGINT,
# Decimal types
decimal_col DECIMAL(10,2),
numeric_col NUMERIC(8,3),
float_col FLOAT,
double_col DOUBLE,
# String types
char_col CHAR(10),
varchar_col VARCHAR(255),
text_col TEXT,
# Date/time types
date_col DATE,
time_col TIME,
datetime_col DATETIME,
timestamp_col TIMESTAMP,
# COMPLEX type
complex_col COMPLEX,
# Binary types
binary_col BINARY(16),
varbinary_col VARBINARY(64),
# Other types
json_col JSON,
enum_col ENUM('A', 'B', 'C')
);
