# Test basic CREATE TABLE syntax with COMPLEX type
# Grammar: CREATE TABLE table_name (column_list)
# This version uses the fully qualified name.

--source include/villagesql/install_complex_extension.inc
--source include/villagesql/binlog_check_begin.inc

CREATE TABLE t1 (
    id INT PRIMARY KEY,
    value vsql_complex.COMPLEX
);

SHOW CREATE TABLE t1;

CREATE TABLE t2 (
    id INT PRIMARY KEY,
    value vsql_complex.complex,
    value2 vsql_complex.COMPLEX
);

SHOW CREATE TABLE t2;

INSERT INTO t1 VALUES (1, '(1.0,2.0)');
INSERT INTO t1 VALUES (2, '(3.5,-1.2)');

SELECT * FROM t1;

INSERT INTO t2 VALUES (1, '(1.0,2.0)', '(-0,-0)');

SELECT * FROM t2;

--source include/villagesql/query_custom_columns.inc

DROP TABLE t1, t2;

--source include/villagesql/query_custom_columns.inc

--source include/villagesql/binlog_check_end.inc

--source include/villagesql/uninstall_complex_extension.inc
