# Test CREATE TABLE AS SELECT with COMPLEX type - basic case
# Grammar: CREATE TABLE table_name AS SELECT ...

# Create source table with COMPLEX data
--source include/villagesql/install_complex_extension.inc
--source include/villagesql/binlog_check_begin.inc

CREATE TABLE source_data (
    id INT PRIMARY KEY,
    measurement COMPLEX,
    name VARCHAR(50)
);

INSERT INTO source_data VALUES (1, '(1.5,2.5)', 'test1');
INSERT INTO source_data VALUES (2, '(3.5,4.5)', 'test2');

# Create table using AS SELECT *
CREATE TABLE t1 AS SELECT * FROM source_data;

SHOW COLUMNS FROM t1;

INSERT INTO t1 VALUES (3, '(5.5,6.5)', 'test3');

SELECT * FROM t1;

DROP TABLE t1;
DROP TABLE source_data;

--source include/villagesql/binlog_check_end.inc
--source include/villagesql/uninstall_complex_extension.inc
