# Test CREATE TABLE with virtual generated column using COMPLEX
# Tests virtual generated column with COMPLEX type expressions

--source include/villagesql/not_implemented_complex_type.inc

--source include/villagesql/install_complex_extension.inc
--source include/villagesql/binlog_check_begin.inc

CREATE TABLE t1 (
    id INT PRIMARY KEY,
    complex_input COMPLEX,
    real_part DOUBLE GENERATED ALWAYS AS (REAL(complex_input)) VIRTUAL
);

SHOW CREATE TABLE t1;

INSERT INTO t1 (id, complex_input) VALUES (1, '(5.5,7.7)');
INSERT INTO t1 (id, complex_input) VALUES (2, '(9.9,11.1)');

SELECT * FROM t1;

DROP TABLE t1;

