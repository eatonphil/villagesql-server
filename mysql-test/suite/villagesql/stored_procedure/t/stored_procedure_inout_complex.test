# Test Stored Procedure with INOUT parameter of COMPLEX type
# Grammar: CREATE PROCEDURE ... (INOUT name COMPLEX) ...

--source include/villagesql/not_implemented_complex_type.inc

--source include/villagesql/install_complex_extension.inc

delimiter //;
CREATE PROCEDURE p1(INOUT val COMPLEX)
BEGIN
  # Simple pass-through logic for now, assuming string representation
  # Ideally this would do math if operations were supported
  SET val = '(9.0,9.0)'; 
END//
delimiter ;//

SET @my_val = '(1.0,2.0)';
CALL p1(@my_val);
SELECT @my_val;

DROP PROCEDURE p1;

--source include/villagesql/uninstall_complex_extension.inc
