# Test Stored Procedure with IN parameter of COMPLEX type
# Grammar: CREATE PROCEDURE ... (IN name COMPLEX) ...

--source include/villagesql/not_implemented_complex_type.inc

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t1 (val COMPLEX);

delimiter //;
CREATE PROCEDURE p1(IN input_val COMPLEX)
BEGIN
  INSERT INTO t1 VALUES (input_val);
END//
delimiter ;//

CALL p1('(1.0,2.0)');

SELECT * FROM t1;

DROP PROCEDURE p1;
DROP TABLE t1;

--source include/villagesql/uninstall_complex_extension.inc
