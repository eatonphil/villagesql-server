# Test LOAD DATA INFILE with COMPLEX type - basic functionality test

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t1 (id INT, val COMPLEX);

# Use tab-separated values (default) to avoid conflict with comma in complex values
--write_file $MYSQLTEST_VARDIR/tmp/complex_basic.dat
1	(1.0,2.0)
2	(3.0,4.0)
EOF

--replace_result $MYSQLTEST_VARDIR MYSQLTEST_VARDIR
--eval LOAD DATA INFILE '$MYSQLTEST_VARDIR/tmp/complex_basic.dat' INTO TABLE t1

SELECT * FROM t1 ORDER BY id;

DROP TABLE t1;
--remove_file $MYSQLTEST_VARDIR/tmp/complex_basic.dat

--source include/villagesql/uninstall_complex_extension.inc
