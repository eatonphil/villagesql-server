# Test LOAD DATA INFILE with COMPLEX type - error handling for bad values

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t1 (id INT, val COMPLEX);

# Create a file with an invalid COMPLEX value
--write_file $MYSQLTEST_VARDIR/tmp/complex_bad.dat
1	(1.0,2.0)
2	not_a_complex_value
3	(3.0,4.0)
EOF

--replace_result $MYSQLTEST_VARDIR MYSQLTEST_VARDIR
--error ER_TRUNCATED_WRONG_VALUE_FOR_FIELD
--eval LOAD DATA INFILE '$MYSQLTEST_VARDIR/tmp/complex_bad.dat' INTO TABLE t1

# Table should be empty since the load failed
SELECT * FROM t1 ORDER BY id;

DROP TABLE t1;
--remove_file $MYSQLTEST_VARDIR/tmp/complex_bad.dat

--source include/villagesql/uninstall_complex_extension.inc
