# Test INSERT INTO ... VALUES ROW(...) with COMPLEX type
# Grammar: INSERT INTO tbl_name VALUES ROW(...)

--source include/villagesql/install_complex_extension.inc

CREATE TABLE t1 (
    id INT PRIMARY KEY,
    val COMPLEX
);

# Insert 30+ rows using explicit ROW constructors with zero variants and negative values
INSERT INTO t1 VALUES
    ROW(1, '(15.0,20.0)'),
    ROW(2, '(-0,0)'),
    ROW(3, '(-7.5,-8.5)'),
    ROW(4, '(0,0)'),
    ROW(5, '(22.0,-13.0)'),
    ROW(6, '(0,-0)'),
    ROW(7, '(-18.0,9.0)'),
    ROW(8, '(3.5,4.2)'),
    ROW(9, '(-0,-0)'),
    ROW(10, '(11.5,-12.5)'),
    ROW(11, '(0,0)'),
    ROW(12, '(-25.0,-30.0)'),
    ROW(13, '(19.0,21.0)'),
    ROW(14, '(-0,0)'),
    ROW(15, '(7.0,-5.0)'),
    ROW(16, '(-14.0,16.0)'),
    ROW(17, '(0,-0)'),
    ROW(18, '(26.0,28.0)'),
    ROW(19, '(-9.5,11.5)'),
    ROW(20, '(-0,-0)'),
    ROW(21, '(33.0,-35.0)'),
    ROW(22, '(0,0)'),
    ROW(23, '(-22.0,24.0)'),
    ROW(24, '(17.5,19.5)'),
    ROW(25, '(0,-0)'),
    ROW(26, '(-31.0,-32.0)'),
    ROW(27, '(29.0,30.0)'),
    ROW(28, '(-0,0)'),
    ROW(29, '(12.0,-14.0)'),
    ROW(30, '(-27.0,28.0)'),
    ROW(31, '(0,0)'),
    ROW(32, '(34.0,-36.0)'),
    ROW(33, '(-0,-0)');

SELECT * FROM t1 ORDER BY id;

# Test ordering by COMPLEX column
SELECT * FROM t1 ORDER BY val, id;

DROP TABLE t1;

